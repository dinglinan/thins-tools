{
  "name": "THINS-TOOLS",
  "short_name": "THINS-TOOLS",
  "background": {
    "scripts": ["lib/unixTimeStamp/main.js","lib/showIP/js/background.js"]
   },
  "chrome_url_overrides": {
    "newtab": "core/newTab.html"
  },
   "description": "desc",
   "homepage_url": "http://www.richdown.com",
   "icons": {
      "16": "assets/icon_16.png",
      "48": "assets/icon_128.png",
      "128": "assets/icon_128.png"
   },
  "browser_action": {
    "default_icon": {
      "19": "assets/icon_19.png",
      "38": "assets/icon_38.png"
    },
    "default_title": "showIP"
  },
   "manifest_version": 2,
  "optional_permissions": [ "topSites", "chrome://favicon/" ],
  "permissions": [
    "tabs", "*://*/*","contextMenus","declarativeContent","tabs", "bookmarks", "management", "unlimitedStorage","webRequest", "\u003Call_urls>"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.0.1",
  "options_page": "lib/showIP/option/options.html",
  "content_scripts":[
    {
      "matches":["*://*/*","http://*/*","https://*/*"],
      "js":["lib/jquery-1.10.2.min.js"]
    },
    {
      "matches":["\u003Call_urls>"],
      "js":["lib/showIP/js/hover-box.js"],
      "run_at": "document_end"
    }
  ],
  "sandbox": {
    "pages": ["lib/editPlusRegCode.html" ]
  },
  "web_accessible_resources": [
    "assets/UNIX_TS_2.png"
  ]
}
